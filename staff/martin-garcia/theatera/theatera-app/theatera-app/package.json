{
    "name": "theatera-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-scripts": "3.2.0",
        "theatera-util": "file:../theatera-util"
    },
    "devDependencies": {
        "jsonwebtoken": "^8.5.1",
        "node-sass": "^4.13.0",
        "theatera-data": "file:../tasks-data"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --runInBand --no-cache",
